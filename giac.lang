    </define-regex>
    
    <define-regex id="unit" extended="true">
      \%{simple-unit} | \(([\*\/]?\%{simple-unit}(\^-?[0-9]+)?)+\)
    </define-regex>
    
    <define-regex id="number" extended="true">
      ([1-9][0-9]* | 0)
      |
      (
      ([0-9]+ e [-+]? [0-9]+ |
       ([0-9]* \. [0-9]+ | [0-9]+ \.)
       (e [-+]? [0-9]+)?) [fl]?
      )
    </define-regex>
    
    <context id="general-number" style-ref="def:number">
    	<match>\b(\%{number}\b|[0-9]+(?=([a-zA-ZαβγΓδΔεζθΘικλΛµνξΞπΠρσΣτυφΦηχψΨωΩ]|\b)))</match>
    </context>
    
    <context id="physical-constant">
      <match>\b_(\%{constant})_\b</match>
      <include>
        <context sub-pattern="1" style-ref="reserved-constant"/>
      </include>
    </context>
    
    <context id="physical-value">
      <match>\b(\%{number})?_(\%{unit})</match>
      <include>
        <context sub-pattern="1" style-ref="def:number"/>
        <context sub-pattern="7" style-ref="physical-unit"/>
      </include>
    </context>
      
    <context id="giac" class="no-spell-check">
      <include>
        <context ref="def:c-like-comment"/>
        <context ref="def:c-like-comment-multiline"/>
        <context ref="def:string"/>
        <context ref="delims"/>
        <context ref="boolean"/>
        <context ref="keyword"/>
        <context ref="option"/>
        <context ref="variable"/>
        <context ref="builtin"/>
        <context ref="physical-constant"/>
        <context ref="physical-value"/>
        <context ref="general-number"/>
        <context ref="reserved-constant"/>
        <context ref="operator"/>
      </include>
    </context>
  </definitions>

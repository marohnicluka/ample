        ozt |       # Troy ounce
        ozUK |      # UK fluid ounce
        P |         # Poise (measures viscosity)
        Pa |        # Pascal
        pc |        # Parsec
        pdl |       # Poundal (force)
        ph |        # Phot
        pk |        # US peck
        psi |       # Pounds per square inch
        liqpt |     # US liquid pint (1_galUS=8_liqpt)
        ptUK |      # UK pint (1_galUK=8_ptUK)
        qt |        # Quart
        rad |       # Radian
        R |         # Roentgen
        rd |        # Rad 1_rd=0.01_Gy
        rod |       # Rod 1_rod=5.029215842_m
        rem |       # Rem
        rpm |       # Revolutions per minute
        s |         # Second
        S |         # Siemens
        sb |        # Stilb
        slug |      # Slug
        sr |        # Steradian
        st |        # Stere
